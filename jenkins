pipeline {
    agent {
         node {
            label 'newnode'
            }
    }
    stages {
        
        stage('vcs') {

            steps {
                git url: 'https://github.com/Praveen-711/nopCommerce-Docs.git' ,
                    branch: 'master'
            }
        }
        stage ('build'){
             steps{

                 sh "dotnet build"
                 sh "dotnet publish"
             }
            }
    }
}